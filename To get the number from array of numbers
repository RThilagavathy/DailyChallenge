#include <stdio.h>

int main()
{
    int n,i,num=0;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        num=num*10 + a[i];
    }
    printf("The actual single digit number formed by all the array elements: %d",num);
    return 0;
}
IP:
4 
6 7 3 1
OP:
6731
