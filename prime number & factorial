/* prime No chking
int main()
{
    int n,f=0,i;
    scanf("%d",&n);
    for(i=2;i<=n/2;i++)
        if(n%i==0)
            f=1;
    if(f==0)
       printf("Prime");
    else
       printf("Not Prime");
}*/
/*factorial
int fact(int n)
{
    if(n==0||n==1)
       return 1;
    return n*fact(n-1);   
}
int main()
{
    int n;
    scanf("%d",&n);
    printf("%d",fact(n));
    return 0;
}*/
